<h1>Title: {{title}}</h1>
<h3>Genre: {{genre}}</h3>
<p>Plot: {{plot}}</p>
{{#each cast}}
    <p>Cast: {{name}}</p>
    <p>Occupation: {{occupation}}</p>
    <p>Catch Phrase: {{catchPhrase}}</p>
{{/each}}
    <a href="/movies/{{_id}}/edit">Edit</a>
    <br><br>
    <form action="/movies/{{_id}}/delete" method="POST">
      <button type="submit">Delete</button>
    </form>